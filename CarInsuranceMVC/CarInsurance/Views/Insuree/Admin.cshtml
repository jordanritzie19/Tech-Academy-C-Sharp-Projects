@using CarInsurance.Models

@model List<Insuree>

@{
    ViewBag.Title = "Admin";
}

<h2 class="text-center">Admin</h2>

<div class="row no-gutters m-3">
    <div class="card col-sm-12 col-md-10 col-lg-6 mx-auto shadow">
        <div class="card-header text-center">
            <h5 class="m-0">Insurees</h5>
        </div>
        <div class="card-body">
            @foreach (var insuree in Model)
            {
                <div class="row mb-3">
                    <div class="col-2">
                        @insuree.FirstName
                    </div>
                    <div class="col">
                        @insuree.LastName
                    </div>
                    <div class="col-4">
                        @insuree.EmailAddress
                    </div>
                    <div class="col-4">
                        @insuree.Quote
                    </div>
                </div>
                <br />
            }
        </div>
    </div>
</div>





